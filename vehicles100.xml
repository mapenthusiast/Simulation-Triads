	<!-- Passenger Car -->
	<vehicleType id="car">
		<description>Standard Passenger Car</description>
		<capacity seats="4" standingRoomInPersons="0"/>
		<length meter="4.5"/>
		<width meter="1.8"/>
		<maximumVelocity meterPerSecond="33.33"/>
		<passengerCarEquivalents pce="1.0"/>
		<networkMode networkMode="car"/>
	</vehicleType>

